{
  "name": "rollup-plugin-generate-file",
  "version": "1.0.0",
  "description": "generate file",
  "type": "module",
  "main": "dist/inedx.cjs",
  "module": "dist/inedx.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts", 
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "keywords": [
    "rollup",
    "plugin",
    "generate",
    "file"
  ],
  "author": "Clarence",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chencaize/rollup-plugin-generate-file"
  },
  "scripts": {
    "build": "vite build && tsc -b"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "@types/node": "^24.0.1"
  }
}